    for (elem of document.querySelectAll('select.form-control')) {
      elem.className = elem.ClassName.replace(new RegExp('(?:^|\\s)'+ 'form-control' + '(?:\\s|$)'), '');
      elem.setAttribute('style', 'width: 100%; ' + (elem.getAttribute('style') || ''))
    }
  }
})();
